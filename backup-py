   # find all documents that have an doi id but not a arXiv

    if d.identifiers is None:
        print "No identifiers found!"
    else:

        if 'doi' not in d.identifiers:
            print "No DOI found!"

        else:
            doi = d.identifiers['doi']

            if doi is not None:
                # print "Found" ,doi

                if 'arxiv' not in d.identifiers:

                    # print d.title
                    # print d.identifiers

                    # look up the arXiv using the DOI id in ADS
                    papers = list(ads.SearchQuery(doi = doi))
                    for paper in papers:
                        print paper.title
                        print paper.identifier


                else:
                    print "DOI not None"
            else:
                print "DOI has a value", doi
